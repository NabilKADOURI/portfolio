<section class="portfolio" id="portfolio">
	<div class="container">
		<h2 class="section-title mb-5">Mes
			<span>projets</span>
		</h2>
		<div class="portfolio-container">
			{% for project in projects %}
				<div class=" mb-5 portfolio-box col-lg-3 ">
					<img src="uploads/{{project.picture}}" alt="{{project.title}}">
					<div class="portfolio-layer">
						<h4>{{project.title}}</h4>
						<a target="_blank" href='{{path('app_project', {id:project.id})}}'>
							<i class='bx bx-link-external'></i>
						</a>
					</div>
				</div>
			{% endfor %}
		</div>
	</div>
</section>
